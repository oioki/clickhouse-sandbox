<yandex>
  <profiles>
    <default>
      <max_threads>1</max_threads>
      <max_memory_usage>10737418240</max_memory_usage>
      <use_uncompressed_cache>1</use_uncompressed_cache>
      <compile>1</compile>
      <compile_expressions>0</compile_expressions>
      <load_balancing>random</load_balancing>
      <send_timeout>60</send_timeout>
      <connect_timeout_with_failover_ms>100</connect_timeout_with_failover_ms>
      <connections_with_failover_max_tries>10</connections_with_failover_max_tries>
      <optimize_move_to_prewhere>1</optimize_move_to_prewhere>
      <background_pool_size>8</background_pool_size>
      <background_schedule_pool_size>8</background_schedule_pool_size>
      <insert_quorum>1</insert_quorum>
      <max_expanded_ast_elements>10000000</max_expanded_ast_elements>
      <mark_cache_min_lifetime>600</mark_cache_min_lifetime>
      <log_profile_events>false</log_profile_events>
      <input_format_defaults_for_omitted_fields>0</input_format_defaults_for_omitted_fields>
      <distributed_aggregation_memory_efficient>true</distributed_aggregation_memory_efficient>
    </default>
  </profiles>
  <users>
    <default>
      <password></password>
      <networks>
        <ip>::/0</ip>
      </networks>
      <profile>default</profile>
      <quota>default</quota>
    </default>
  </users>
  <quotas>
    <default>
      <interval>
        <duration>3600</duration>
        <queries>0</queries>
        <errors>0</errors>
        <result_rows>0</result_rows>
        <read_rows>0</read_rows>
        <execution_time>0</execution_time>
      </interval>
    </default>
  </quotas>
</yandex>
